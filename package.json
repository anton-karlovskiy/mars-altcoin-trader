{
  "name": "mars-altcoin-trader",
  "version": "1.0.0",
  "description": "Advanced DeFi trading bot supporting Uniswap V2, Uniswap V3, and Raydium DEXs with automated trading strategies, price analysis, and multi-chain support for Ethereum and Solana networks.",
  "main": "dist/index.js",
  "scripts": {
    "ts-prepare": "ts-patch install -s",
    "start:dev": "npm run ts-prepare && nodemon ./src/index.ts",
    "start:prod": "node ./dist/index.js",
    "build": "tsc && tsc-alias",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "defi",
    "trading-bot",
    "uniswap",
    "raydium",
    "ethereum",
    "solana",
    "crypto-trading",
    "automated-trading",
    "dex",
    "amm",
    "liquidity",
    "trading-strategies",
    "web3",
    "blockchain",
    "cryptocurrency",
    "trading-automation",
    "price-analysis",
    "slippage-protection",
    "multi-chain",
    "typescript"
  ],
  "author": "Anton K. <antonkarlovskiy@outlook.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.26",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.51",
    "@uniswap/sdk-core": "^4.2.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v2-sdk": "^4.3.0",
    "@uniswap/v3-sdk": "^3.11.0",
    "bs58": "^5.0.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "express": "^4.19.2"
  },
  "engines": {
    "node": "18.x.x"
  }
}
